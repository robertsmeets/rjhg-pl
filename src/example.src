extern fopen   pp-p      ;
extern fclose  p-p       ;
extern fprintf pp+i      ;

proc main()
{
   fp = fopen("file.txt","w");
   if (fp==null)
   {
      print "could not open the file";
      return;
   };
   i = 1;
   while i <32000 
   {
      fprintf(fp,"%d and %s\n",i,"BLA");
      i=i+1;
   };
   fclose(fp);
}

