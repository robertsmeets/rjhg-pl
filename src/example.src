extern fopen   pp-p      ;
extern fclose  p-p       ;
extern fprintf pp+i      ;

proc main()
{
   fp = fopen("file.txt","w");
   if (fp == 0)
   {
      print "Something went wrong";
   };
   i = 1;
   while i < 10
   {
      print i ;
      fprintf(fp,"%d and %s",i,"BLA");
      i=i+1;
   };
   fclose(fp);
}



