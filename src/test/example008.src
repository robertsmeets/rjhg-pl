//
// test instance variables in combination with parameters
//
class myclass
{
    a,b,c,d,e
}

proc main(a)
{
   m = myclass();
   f = m.mymethod();
   m.anothermethod(66,77,88);
}


method myclass.mymethod()
{
   a=11;
   b=22;
   c=33;
   d=44;
   e=55
}

method myclass.anothermethod(x,y,z)
{
   print a;
   print b;
   print c;
   print d;
   print e;
   print x;
   print y;
   print z;
}
